{
  "id": "${artifactId}-${version}",
  "name": "Edge NCIP",
  "provides": [],
  "requires": [
    {
      "id": "ncip",
      "version": "1.0"
    },
    {
      "id": "login",
      "version": "5.0 6.0"
    }
  ],
  "permissionSets": [],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 357913941,
        "PortBindings": { "8081/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      }
    ]
  }
}
